<h3 class="page-header">商家管理</h3>

<%= index_for [ :admin, @users ], class: "table table-hover data-tables" do |i| %>
  <%= i.attribute :avatar do |user| %>
    <%= image_tag(user.avatar.url(:thumb)) %>
  <% end %>
  <%= i.attribute :nickname %>
  <%= i.attribute :mobile %>
  <%= i.attribute :weixin_openid %>

  <%= i.actions :show, :destroy %>
<% end %>
