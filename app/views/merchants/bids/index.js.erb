<% if @order.canceled? %>
  clearInterval(interval);
  alert("非常抱歉，订单超时，您没有选择技师\r\n本次服务取消\r\n点击确认再来一次");
  window.location.href = "/merchants/orders/new";
<% else %>
  var index = "<%= j render "index" %>";
  var $content = $("#content");
  if ($content.html() !== index) $content.html(index);
<% end %>
